{
  "name": "sounderkit",
  "title": "SounderKit",
  "description": "SounderKit is a lightweight, modular front-end (CSS only) framework for developing fast and great looking web interfaces. This framework is developed & maintained by the Sounder.fm team which we wanted to share with the world.",
  "version": "1.0.0",
  "homepage": "https://sounder.fm",
  "author": {
    "name": "Sounder",
    "email": "dev@sounder.fm",
    "url": "https://sounder.fm"
  },
  "main": "dist/js/skit.js",
  "style": "dist/css/skit.css",
  "license": "MIT",
  "keywords": [
    "css",
    "sass",
    "flexbox",
    "responsive",
    "framework"
  ],
  "devDependencies": {
    "archiver": "^3.0.0",
    "autoprefixer": "^9.4.7",
    "buble": "^0.19.6",
    "buble-loader": "^0.5.1",
    "camelcase": "^5.0.0",
    "circular-dependency-plugin": "^5.0.2",
    "clean-css-cli": "^4.2.1",
    "dateformat": "^3.0.3",
    "eslint": "^5.14.1",
    "glob": "^7.1.3",
    "inquirer": "^6.2.2",
    "make-dir-cli": "^2.0.0",
    "minimist": "^1.2.0",
    "node-sass": "^4.11.0",
    "postcss-cli": "^6.1.1",
    "raw-loader": "^1.0.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.2.3",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-import-alias": "^1.0.5",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-replace": "^2.1.0",
    "semver": "^5.6.0",
    "shortid": "^2.2.14",
    "svgo": "^1.2.0",
    "uglify-js": "^3.4.9",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "scripts": {
    "build": "npm run build-css && npm run build-js",
    "build-css": "npm run build-clean && npm run build-sass && npm run build-autoprefix && npm run build-cleancss",
    "build-autoprefix": "postcss --use autoprefixer --map false --output dist/css/skit.css dist/css/skit.css",
    "build-cleancss": "cleancss -o dist/css/skit.min.css dist/css/skit.css",
    "build-clean": "rimraf css",
    "build-sass": "node-sass --output-style expanded --source-map true src/sass/skit.sass dist/css/skit.css",
    "build-js": "node build/build",
    "deploy": "npm run build",
    "eslint": "eslint src/js",
    "eslint-fix": "eslint --fix src/js",
    "start": "npm run build-sass -- --watch"
  }
}
